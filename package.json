		"lint:css": "stylelint --fix --color --cache --max-warnings=0 --quiet-deprecation-warnings \"**/*.{scss,css}\"",
		"webpack:validate": "webpack configtest",
		"format": "prettier --check -w --cache .",
		"prepare": "husky install"
	},
